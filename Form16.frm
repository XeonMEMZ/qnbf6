Private Sub Timer1_Timer()
If Me.Top + 250 >= Form2.Height + 615 * 12 And speed > 0 Then
 If Me.Top >= Form2.Height + 615 * 12 + 2000 Then
  Me.Top = Me.Top - speed
 Else
  Me.Top = Me.Top - speed
  speed = speed - 18
 End If
Else
 speed = 1
 Me.Top = Form2.Height + 615 * 12
 Timer1.Enabled = False
End If
End Sub

Private Sub Timer2_Timer()
If kbdh = True Then
 speed = 1
 Me.Width = 1805
 Me.Left = Form2.Left
 Timer3.Enabled = False
 Timer2.Enabled = False
End If
If speed > 0 Then
 If Me.Left - speed >= leftt + 1805 - 9510 + (7705 / 3) Then
  Me.Width = Me.Width + speed
  Me.Left = Me.Left - speed
  speed = speed + 5
 Else
  Me.Width = Me.Width + speed
  Me.Left = Me.Left - speed
  speed = speed - 9.5
 End If
Else
 speed = 1
 mode = True
 Me.Width = 9510
 Me.Left = leftt + 1805 - 9510
 Timer2.Enabled = False
End If
End Sub

Private Sub Timer3_Timer()
If kbdh = True Then
 speed = 1
 Me.Width = 1805
 Me.Left = Form2.Left
 Timer2.Enabled = False
 Timer3.Enabled = False
End If
If speed > 0 Then
 If Me.Left - speed <= leftt + 1805 - 9510 + (7705 / 2.5) Then
  Me.Width = Me.Width - speed
  Me.Left = Me.Left + speed
  speed = speed + 6
 Else
  Me.Width = Me.Width - speed
  Me.Left = Me.Left + speed
  speed = speed - 5
 End If
Else
 speed = 1
 mode = False
 Me.Width = 1805
 Me.Left = leftt
 Timer3.Enabled = False
End If
End Sub

Private Sub Timer4_Timer()
Open "data\cy" & cyn & ".txt" For Input As #1
Line Input #1, cy
Close #1
If cyc = False Then
 Dim x As Integer
 Dim myval As Integer
 myval = ExtractIcon(hInst, cy, -1)
 Picture1.Cls
 hIcon = ExtractIcon(0, cy, 0)
 x = DrawIcon(Picture1.hDC, 5, 5, hIcon)
 Picture1.Picture = Picture1.Image
 SavePicture Picture1.Picture, "icon\" & cyn & ".ico"
 If cyn = 1 Then
  Image1.Picture = LoadPicture("icon\1.ico")
 ElseIf cyn = 2 Then
  Image2.Picture = LoadPicture("icon\2.ico")
 ElseIf cyn = 3 Then
  Image3.Picture = LoadPicture("icon\3.ico")
